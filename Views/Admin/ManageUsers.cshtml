@model IEnumerable<DatSan.Models.TaiKhoan>

@{
    ViewBag.Title = "ManageUsers";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
@section SanCauLong5{
<h2>Quản lý người dùng</h2>
<table class="table">
    <thead>
        <tr>
            <th>Tên đăng nhập</th>
            <th>Mật Khẩu</th>
            <th>Email</th>
            <th>Ngày tạo</th>
            <th>Hành động</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.TenDangNhap</td>
                <td>@user.MatKhau</td>
                <td>@user.Email</td>
                <td>@user.NgayTao</td>
                <td>
                    <a href="#">Chỉnh sửa</a> |
                    <a href="#">Xóa</a>
                </td>
            </tr>
        }
    </tbody>
</table>
}